<% @articles.each do |article| %>
  <h2><%= article.title %></h2>
  <p><%= article.content %></p>

<%= link_to "Edit", edit_article_path(article) %>
<%= link_to "Create", new_article_path(article) %>
<%= link_to "Delete", article_path(article),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
<% end %>
